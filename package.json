{
  "name": "simple-api",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "cross-env NODE_ENV=development nodemon  --exec babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "unseed": "npx sequelize-cli db:seed:undo",
    "unmigrate": "npx sequelize-cli db:migrate:undo",
    "unmigrate:all": "npx sequelize-cli db:migrate:undo:alll",
    "prepare": "husky install",
    "test": "jest tests/",
    "lint": "eslint --ext .js src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jest": "^28.1.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.10",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/node": "^7.16.8",
    "@babel/preset-env": "^7.16.11",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.0.5"
  },
  "description": "Simple api made with NodeJS and Sequelize"
}
